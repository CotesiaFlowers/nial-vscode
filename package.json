{
  "name": "nial-language",
  "displayName": "Nial language",
  "description": "Nial array programming language support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "nial",
      "aliases": ["Nial","nial"],
      "extensions": [".ndf"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "nial",
      "scopeName": "source.nial",
      "path": "./syntaxes/nial.tmLanguage.json"
    }]
  }
}
